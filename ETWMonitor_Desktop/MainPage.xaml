<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:EtwMonitor.Desktop"
    xmlns:components="clr-namespace:EtwMonitor.Desktop.Components"
    x:Class="EtwMonitor.Desktop.MainPage"
    Title="ETW Ducky">

    <BlazorWebView x:Name="blazorWebView" HostPage="wwwroot/index.html">
        <BlazorWebView.RootComponents>
            <RootComponent Selector="#app" ComponentType="{x:Type components:Root}" />
        </BlazorWebView.RootComponents>
    </BlazorWebView>
</ContentPage>
